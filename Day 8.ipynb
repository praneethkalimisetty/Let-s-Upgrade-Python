    "def flip(str): \n",
    "    sum = 0\n",
    "    n = len(str) \n",
    "    for i in range( 0, n ): \n",
    "        sum += int(str[i]) - int('0') \n",
    "  \n",
    "    return (sum == n - 1 or sum == 1) \n",
    "\n",
    "n = input()\n",
    "if flip(n):\n",
    "    print(\"YES\")\n",
    "else:\n",
    "    print(\"NO\")"
   ]
  }
